// out: css_stylus.css, compress: true

border-radius()
  -webkit-border-radius: arguments
  -moz-border-radius: arguments
  border-radius: arguments

body{
  color: tomato;
  h1{
    font-size: 10px;
    border-radius: 15px, 10px;
  }
  h2{
    border-radius: 10px, 5px;
  }
  button{
    border-radius: 5px, 100px;
  }
}
